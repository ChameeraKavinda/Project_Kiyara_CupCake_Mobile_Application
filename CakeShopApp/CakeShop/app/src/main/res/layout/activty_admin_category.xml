<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/cc"

    android:layout_height="match_parent">

    <Button
        android:id="@+id/btnLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:text="Log Out"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.984"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Cup Cakes"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnCup"
        app:layout_constraintStart_toStartOf="@+id/btnCup"
        app:layout_constraintTop_toBottomOf="@+id/btnCup" />

    <ImageButton
        android:id="@+id/btnCup"
        android:layout_width="90dp"
        android:layout_height="105dp"
        android:layout_marginStart="48dp"


        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/cak" />

    <ImageButton
        android:id="@+id/btnBirth"
        android:layout_width="99dp"
        android:layout_height="107dp"
        android:layout_marginStart="8dp"

        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/btnCup"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/birthg" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="89dp"
        android:layout_height="21dp"
        android:layout_marginTop="4dp"
        android:text="Birthday cake"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnBirth"
        app:layout_constraintStart_toStartOf="@+id/btnBirth"
        app:layout_constraintTop_toBottomOf="@+id/btnBirth" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="349dp"
        android:layout_height="72dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="144dp"
        android:text="Categaries Of Cake"
        android:textColor="#414F9C"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btnAns"
        android:layout_width="105dp"
        android:layout_height="106dp"
        android:layout_marginStart="8dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/btnBirth"
        app:layout_constraintTop_toTopOf="@+id/btnBirth"
        app:srcCompat="@drawable/anii" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="85dp"
        android:layout_height="40dp"
        android:text="Anniverssary Cake"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnAns"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="@+id/btnAns"
        app:layout_constraintTop_toBottomOf="@+id/btnAns" />

    <ImageButton
        android:id="@+id/btnVal"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="68dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="@+id/btnCup"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btnCup"
        app:layout_constraintTop_toBottomOf="@+id/btnCup"
        app:srcCompat="@drawable/val" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="106dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:text="Valantines Cake"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnVal"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnVal"
        app:layout_constraintTop_toBottomOf="@+id/btnVal" />

    <ImageButton
        android:id="@+id/btnGrad"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="64dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/btnVal"
        app:layout_constraintTop_toBottomOf="@+id/btnBirth"
        app:srcCompat="@drawable/gra" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="98dp"
        android:layout_height="68dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="8dp"
        android:text="Graduation Cake"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnGrad"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/btnGrad" />

    <ImageButton
        android:id="@+id/btnOth"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="64dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/btnGrad"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        app:srcCompat="@drawable/cel" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="93dp"
        android:layout_height="42dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:text="Other Celebrations"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/btnOth"
        app:layout_constraintHorizontal_bias="0.758"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/btnOth" />

    <LinearLayout
        android:layout_width="406dp"
        android:layout_height="25dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="-1dp">

        <LinearLayout
            android:layout_width="407dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/cart"
                android:layout_width="170dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/white"
                app:srcCompat="@drawable/baseline_add_shopping_cart_24" />

            <ImageButton
                android:id="@+id/acc"
                android:layout_width="208dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@color/white"
                app:srcCompat="@drawable/baseline_account" />
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>